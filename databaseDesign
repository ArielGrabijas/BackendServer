

CREATE TABLE fellowshipmember (
	id SERIAL PRIMARY KEY,
	joined TIMESTAMP,
	name TEXT NOT NULL,
	race TEXT,
	weapontype TEXT
)

INSERT INTO fellowshipmember VALUES (
	DEFAULT,
	now(),
	'Legolas',
	'elf',
	'bow and arrows'
)

SELECT * FROM fellowshipmember

CREATE TABLE authorities (
	id SERIAL PRIMARY KEY,
	role TEXT NOT NULL
)

CREATE TABLE custom_user (
	id SERIAL PRIMARY KEY,
	username TEXT NOT NULL,
	password TEXT NOT NULL,
	enabled BOOLEAN
)